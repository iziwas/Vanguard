<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="vanguard_state_tree">
            <field name="name">vanguard_state_tree</field>
            <field name="model">vanguard.state</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="code_response" />
                    <field name="type" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="vanguard_state_form">
            <field name="name">vanguard_state_form</field>
            <field name="model">vanguard.state</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name" />
                    <field name="code_response" />
                    <field name="type" />
                </form>
            </field>
        </record>
    </data>
</odoo>